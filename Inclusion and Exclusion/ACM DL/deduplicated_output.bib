@inproceedings{10.1145/3659154.3659169,
 abstract = {Phishing has become one of the major security threats to Web users, and has caused more financial and privacy loss each year. Nowadays, most anti-phishing research focuses on phishing detection, the technical side while ignoring the other side, the users. Since the Internet is in fact the interaction between people and the network, to comprehensively combat phishing, the anti-phishing training of Web users should be strengthened. This paper analyzes some representative works on why people get phished and anti-phishing training methods, learns some education and psychology theories, studies recent phishing statistics reports, and then proposes several design principles, so that a solid foundation is laid for the design of the training method, including training contents, content arrangement, and training form. Then an online interactive training tool is designed, which covers three phishing forms\textemdash  phishing websites, messages, and emails. To evaluate the effectiveness of the training method, we organized volunteers to actually participate in the training, and carried out pre- and post-tests to evaluate the training method. The proposed method is proved practical and statistically effective through the paired samples t-test analysis.},
 address = {New York, NY, USA},
 author = {Liu, Dong-Jie and Lee, Jong-Hyouk},
 booktitle = {Proceedings of the 2023 International Conference on Intelligent Computing and Its Emerging Applications},
 doi = {10.1145/3659154.3659169},
 isbn = {9798400709050},
 keywords = {Phishing, Phishing websites identification, Anti-phishing Education},
 location = {},
 numpages = {7},
 pages = {59\textendash 65},
 publisher = {Association for Computing Machinery},
 series = {ICEA \textquotesingle 23},
 title = {{T}he {D}esign and {E}valuation of an {O}nline {I}nteractive {A}nti-phishing {T}raining {M}ethod},
 url = {https://doi.org/10.1145/3659154.3659169},
 year = {2024}
}

