 publisher = {Association for Computing Machinery},
 series = {ICEGOV \textquotesingle 23},
 title = {{I}nfluence of {I}CT governance on cyber security: a {G}eneralized {M}ethod {M}oment ({G}MM) approach in {A}sia},
 url = {https://doi.org/10.1145/3614321.3614328},
